nums = [] 

num1 = int(input("enter a number...."))
num2 = int(input("enter another number..."))
num3 = int(input("enter another number..."))
num4 = int(input("enter another number..."))

nums.append(num1)
nums.append(num2)
nums.append(num3)


clarity = str(input("do you want the last numnber you entered, y or n...")) 
  
if clarity == "n":
  print(nums)

else: 
  nums.append(num4)
  print(nums)
